FROM ORDERS 
WHERE (amt < 1000 OR NOT (odate = '1990-10-03' AND cnum >2003));

SELECT * 
FROM ORDERS
WHERE NOT((odate = '1990-10-03' OR snum>1006) AND amt>=1500);

